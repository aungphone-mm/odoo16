<?xml version="1.0" encoding="utf-8"?>
<odoo>
    <data>
        <record id="seq_reservation" model="ir.sequence">
            <field name="name">hms.reservation</field>
            <field name="code">hms.reservation</field>
            <field name="prefix"></field>
            <field name="padding">6</field>
            <field name="company_id" eval="False"/>
        </record>

        <record id="seq_hms_folio" model="ir.sequence">
            <field name="name">HMS Folio Sequence</field>
            <field name="code">hms.folio</field>
            <field name="prefix">FO/%(year)s/%(month)s</field>
            <field name="padding">5</field>
            <field name="company_id" eval="False"/>
        </record>
    </data>
</odoo>